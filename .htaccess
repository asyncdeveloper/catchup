
RewriteEngine on

RewriteRule ^page/([0-9]+)/?$ pag.php?page=$1 [NC]
RewriteRule ^post/([0-9a-zA-Z_-]+)/?$ post.php?url=$1 [NC]
RewriteRule ^category/([1-9]+)/([0-9a-zA-Z_-]+)/?$ post-search.php?id=$1&url=$2 [NC]
RewriteRule ^category/([0-9]+)/([0-9a-zA-Z_-]+)/page/([0-9]+)?$ page_category.php?id=$1&url=$2&page=$3 [NC]

